{
    "manifestVersion": 1,
    "id": "xmlGate",
    "name": "XMLGate",
    "version": "0.0.1",
    "publisher": "RomainTeuleBASF",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Tools for checking an attribute value for filtered xml files in a folder.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/xml.png"        
    },
    "files": [
        {
